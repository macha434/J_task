#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(void)
{
  int val;
  int i, a, b, c, d[10],e,f,g,h,j,k;

   srand (time(NULL));
    
  for (i = 0; i < 10; i++){
    d[i] = rand()%6+1;
  }

  i = 0;
    
  for (i = 0; i < 60000; i++){
    val = rand() % 6 + 1;

    switch (a){
    case 1:
      e = e + 1;
      break;
    case 2:
      f = f + 1;
      break;
    case 3:
      g = g + 1;
      break;
    case 4:
      h = h + 1;
      break;
    case 5:
      j = j + 1;
      break;
    case 6:
      k = k + 1;
      break;
    case 7:
      f = f + 1;
      break;
    }
  }
  printf("1 = %d\n2 = %d\n3 = %d\n4 = %d\n5 = %d\n6 = %d\n",e,f,g,h,j,k);
  return 0;
}
