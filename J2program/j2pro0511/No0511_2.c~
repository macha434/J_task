#include <stdio.h>

int main(void)
{
  int a=0, b=0, c=0, d=0;
  printf("時間を入力してください！\n時間:");
  scanf("%d",&a);
    printf("分  :");
  scanf("%d",&b);
  printf("秒  :");
  scanf("%d",&c);


  if(a<24){
    a = a*3600;
  }
  if(b<60){
    b = b*60;
  }
  if(c<60){
  d = a+b+c;
  }
  printf("現在の時間の経過した秒は%dです！\n",d);

  return 0;
}
